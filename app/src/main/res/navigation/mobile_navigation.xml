<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.skin.skinapp2.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_history"
        android:name="com.skin.skinapp2.ui.gallery.GalleryFragment"
        android:label="History"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_treatment"
        android:name="com.skin.skinapp2.ui.slideshow.SlideshowFragment"
        android:label="Treatment"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_hospitals"
        android:name="com.skin.skinapp2.ui.hospital.MapsFragment"
        android:label="Hosptial"
        tools:layout="@layout/fragment_maps"/>

    <fragment
        android:id="@+id/nav_about"
        android:name="com.skin.skinapp2.ui.about.AboutFragment"
        android:label="About Us"
        tools:layout="@layout/about_fragment"/>

    <fragment
        android:id="@+id/nav_privacy"
        android:name="com.skin.skinapp2.ui.privacy.PrivacyFragment"
        android:label="Privacy Policy"
        tools:layout="@layout/privacy_fragment"/>
</navigation>